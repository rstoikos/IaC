{
	"Version": "2012-10-17",
	"Statement": [{
			"Effect": "Allow",
			"Action": "iam:CreateServiceLinkedRole",
			"Resource": "arn:aws:iam::*:role/aws-service-role/*"
		}, {
			"Effect": "Allow",
			"Action": [
				"ec2:DescribeAccountAttributes"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"ec2:*",
				"autoscaling:*",
				"elasticloadbalancing:*",
				"codedeploy:*",
				"rds:*"
			],
			"Resource": "*"
		}
	]

}
